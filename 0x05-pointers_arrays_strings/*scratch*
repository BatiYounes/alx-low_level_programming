#include "main.h"

/**
 * main - Entry point of the program.
 *
 * Return: Always 0.
 */
int main(void)
{
    char *str;

    str = "I do not fear computers. I fear the lack of them - Isaac Asimov";
    _puts(str);

    str = "My School";
    print_rev(str);

    str = "Holberton";
    rev_string(str);

    str = "0123456789";
    puts2(str);

    return (0);
}
